{
  "Comment": "Run a single transfer from the source endpoint, to the destination endpoint",
  "StartAt": "RunTransfer",
  "States": {
    "RunTransfer": {
      "Comment": "Start the Transfer",
      "Type": "Action",
      "Resource": "http://localhost:5000/Transfer",
      "InputPath": "$.TransferSpec",
      "ResultPath": "$.TransferResult",
      "RunAs": "User",
      "Next": "TransferWait"
    },
    "TransferWait": {
      "Comment": "Wait for a Transfer Complete",
      "Type": "ActionWait",
      "ResultPath": "$.TransferResult",
      "End": true
    }
  }
}
